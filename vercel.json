{
  "version": 2,
  "builds": [
    {
      "src": "simple-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simple-server.js"
    }
  ],
  "functions": {
    "simple-server.js": {
      "maxDuration": 30
    }
  },
  "ignoreCommand": "if [ \"$VERCEL_ENV\" = \"production\" ]; then exit 1; else exit 0; fi"
}
